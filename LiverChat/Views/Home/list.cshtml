@model IEnumerable<LiverChat.Models.LiverModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>list</title>
</head>
<body>
  
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Statecount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.State)
            </th>
            <th>count</th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Statecount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.State)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.State.Count())
            </td>
         
        </tr>
    }
    
    </table>
</body>
</html>
